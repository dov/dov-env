XTerm*VT100*translations:       #override \
        Shift<Key>Prior:scroll-back(1,page)\n\
        Shift<Key>Next:scroll-forw(1,page)\n\
        Shift<Key>F29:scroll-back(1,page)\n\
        Shift<Key>F35:scroll-forw(1,page)\n\
	<Key>F35: string("0x1b") string("[5~") \n\
	<Key>F29: string("0x1b") string("[6~") \n\
	Ctrl<Key>Right: string("0x1b") string("[a") \n\
	Ctrl<Key>Left: string("0x1b") string("[b") \n\
	Ctrl<Key>Up: string("0x1b") string("[d") \n\
	Ctrl<Key>Down: string("0x1b") string("[e") \n\
	Ctrl<Key>BackSpace: string("0x1b") string("[c")


XTerm*VT100*font: LucidaSansTypewriter-Bold-14
XTerm*VT100*scrollBar: true
XTerm*VT100*scrollLines: 1000
XTerm*Locale: no
